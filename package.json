{
  "name": "zrbecker-web-client",
  "version": "0.0.1",
  "description": "A reactjs front end for website.",
  "repository": "zrbecker/zrbecker-website",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "compile-client": "webpack",
    "compile-server": "babel server-src -d dist -s",
    "start-dev": "nodemon . -w dist -i dist/public",
    "postinstall": "npm run compile-client && npm run compile-server"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "express-force-domain": "0.0.6",
    "express-sslify": "^1.2.0",
    "mongodb": "^2.2.33",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^2.5.0"
  }
}
